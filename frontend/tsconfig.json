{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJs",
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "strict": true,		
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}